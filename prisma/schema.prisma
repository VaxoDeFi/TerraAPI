// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRESQL_ADDON_URI")
}

model CoinAPI {
  id                Int     @default(autoincrement())
  idRank            String
  rank              Int
  symbol            String  @unique
  name              String?
  supply            String
  maxSupply         String?
  marketCapUsd      String
  volumeUsd24Hr     String?
  priceUsd          String?
  changePercent24Hr String?
  vwap24Hr          String?
  explorer          String?
  icon              String?
}

model Messari {
  id                      String
  symbol                  String  @unique
  rank                    Int
  icon                    String?
  slug                    String
  name                    String
  contract_addresses      String?
  priceUsd                Int?
  priceBtc                Int?
  priceEth                Int?
  percentageChange1HrUsd  Int?
  percentageChange24HrUsd Int?
  currentMarketcap        Int?
  vol24HrUsd              Int?
  realVol24HrUsd          Int?
  athUsd                  Int?
  athTimestamp            Int?
  percentageDownFromAth   Int?
  category                String?
  sector                  String?
  nextHalvingDate         Int?
  genesisBlockDate        Int?
  tokenUsage              String?
  consensusAlgorithm      String?
}

model CoinGeckoList {
  id        String
  symbol    String @unique
  name      String
  platforms String
}
