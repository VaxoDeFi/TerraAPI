{
  "name": "vaxoapp",
  "version": "1.0.0",
  "description": "Express Vaxo App",
  "type": "module",
  "scripts": {
    "start": "node --es-module-specifier-resolution=node dist/index.js",
    "build": "babel src --out-dir dist --extensions .ts",
    "inspect": "kill-port --port 8080 && node --inspect=0.0.0.0:8080 --es-module-specifier-resolution=node dist/index.js",
    "start:dev": "nodemon --watch src --exec babel-node src --source-maps",
    "prestart": "yarn build",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "prettier": "prettier --write **/*.{js,jsx,json,yml,md,ts,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VaxoDeFi/TerraAPI.git"
  },
  "private": true,
  "author": "Guillaume Garcia <guigarciafr@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VaxoDeFi/TerraAPI/issues"
  },
  "homepage": "https://github.com/VaxoDeFi/TerraAPI#readme",
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "axios": "^0.25.0",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "kill-port": "^2.0.0",
    "mongoose": "^6.2.1",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.0",
    "pg": "^8.7.3",
    "pm2": "^5.1.2",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.0",
    "@babel/node": "^7.16.8",
    "@babel/plugin-syntax-import-assertions": "^7.17.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/register": "^7.17.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node-cron": "^3.0.1",
    "@types/pg": "^8.6.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "tap": "^16.2.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "engines": {
    "yarn": "^1.22.5"
  }
}
