{
  "name": "terraapi",
  "version": "1.0.0",
  "description": "Express Terra API",
  "scripts": {
    "start": "node dist",
    "prestart": "yarn build",
    "lint": "eslint src test",
    "lint:fix": "eslint src test --fix",
    "build": "rimraf dist && babel src --copy-files --out-dir dist --source-maps",
    "start:dev": "nodemon --watch src --exec babel-node src --source-maps",
    "prettier": "prettier --write **/*.{js,json,yml,md}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VaxoDeFi/TerraAPI.git"
  },
  "keywords": [
    "terra",
    "api"
  ],
  "private": true,
  "author": "Guillaume Garcia <guigarciafr@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VaxoDeFi/TerraAPI/issues"
  },
  "homepage": "https://github.com/VaxoDeFi/TerraAPI#readme",
  "dependencies": {
    "axios": "^0.25.0",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "mongodb": "^4.3.1",
    "mongodb-memory-server": "^8.2.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.0",
    "pm2": "^5.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.0",
    "@babel/node": "^7.16.8",
    "@babel/register": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1"
  },
  "engines": {
    "node": ">= 12.13.0",
    "npm": ">= 6.12.0",
    "yarn": ">= 1.20.0"
  }
}
